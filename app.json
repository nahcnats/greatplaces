{
  "expo": {
    "name": "Great Places",
    "slug": "great-places",
    "privacy": "public",
    "sdkVersion": "40.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "tk.stanleychan.greatplaces",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "tk.stanleychan.great_places",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/places-adaptive.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
         "CAMERA", 
         "ACCESS_FINE_LOCATION",
         "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCGeMoZC9uoLTX2QkuLt8RxAlNgPcA-mv8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
